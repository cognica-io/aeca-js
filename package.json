{
  "name": "@cognica/client",
  "description": "Javascript client for Cognica database",
  "version": "0.1.0",
  "homepage": "https://cognica.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/cognica-io/cognica-js.git"
  },
  "bugs": {
    "url": "https://forum.cognica.io"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist/index.js",
    "/dist/index.mjs",
    "/dist/index.d.ts"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "generate:proto": "./src/proto/script/generate.sh",
    "build": "tsup src/index.ts --clean --format cjs,esm --dts --minify --platform neutral",
    "test": "jest"
  },
  "keywords": [
    "Cognica",
    "key-value",
    "document store",
    "full-text search",
    "vector search",
    "search engine",
    "database",
    "deep learning",
    "NLP",
    "natural language processing",
    "transformer",
    "vector embedding"
  ],
  "author": "finn@cognica.io",
  "license": "Apache-2.0",
  "devDependencies": {
    "@protobuf-ts/protoc": "^2.9.3",
    "@types/jest": "^29.5.12",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-proto": "^1.167.3",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.0",
    "apache-arrow": "^15.0.0",
    "long": "^5.2.3",
    "parquet-wasm": "0.6.0-beta.2",
    "protobufjs": "^7.2.6"
  }
}
