{
  "name": "@aeca/client",
  "description": "Javascript client for Aeca database",
  "version": "0.1.3",
  "homepage": "https://aeca.ai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cognica-io/aeca-js.git"
  },
  "bugs": {
    "url": "https://forum.aeca.ai"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist/index.js",
    "/dist/index.cjs",
    "/dist/index.d.ts"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "generate:proto": "./src/proto/script/generate.sh",
    "build": "tsup",
    "build:pack": "pnpm run build && pnpm pack",
    "test": "jest"
  },
  "keywords": [
    "Aeca",
    "Cognica",
    "key-value",
    "document store",
    "full-text search",
    "vector search",
    "search engine",
    "database",
    "deep learning",
    "NLP",
    "natural language processing",
    "transformer",
    "vector embedding"
  ],
  "author": "finn@aeca.ai",
  "license": "Apache-2.0",
  "devDependencies": {
    "@grpc/grpc-js": "^1.10.1",
    "@protobuf-ts/protoc": "^2.9.3",
    "@types/jest": "^29.5.12",
    "eslint": "^8.57.0",
    "jest": "^29.0",
    "long": "^5.2.3",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "protobufjs": "^7.2.6",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-proto": "^1.167.8",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "apache-arrow": "^15.0.0",
    "parquet-wasm": "^0.6.1"
  }
}
