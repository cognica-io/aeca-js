{
  "name": "@cognica/client",
  "description": "Javascript client for Cognica database",
  "version": "0.1.7",
  "homepage": "https://cognica.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cognica-io/cognica-js.git"
  },
  "bugs": {
    "url": "https://forum.cognica.io"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist/index.js",
    "/dist/index.cjs",
    "/dist/index.d.ts"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "generate:proto": "./src/proto/script/generate.sh",
    "build": "tsup",
    "build:pack": "pnpm run build && pnpm pack",
    "test": "jest"
  },
  "keywords": [
    "Cognica",
    "key-value",
    "document store",
    "full-text search",
    "vector search",
    "search engine",
    "database",
    "deep learning",
    "NLP",
    "natural language processing",
    "transformer",
    "vector embedding"
  ],
  "author": "finn@cognica.io",
  "license": "Apache-2.0",
  "devDependencies": {
    "@grpc/grpc-js": "^1.10.9",
    "@protobuf-ts/protoc": "^2.9.4",
    "@types/jest": "^29.5.12",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "long": "^5.2.3",
    "prettier": "^3.3.2",
    "prettier-eslint": "^16.3.0",
    "protobufjs": "^7.3.2",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "ts-proto": "^1.180.0",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@apache-arrow/esnext-esm": "^16.1.0",
    "parquet-wasm": "^0.6.1"
  }
}
